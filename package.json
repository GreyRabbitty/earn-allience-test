{
  "name": "webhook",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "husky install",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "run() { nest start --watch $1 | pino-pretty -S; }; run",
    "start:debug": "run() { nest start --debug --watch $1 | pino-pretty; }; run",
    "start:repl": "run() { nest start --entryFile repl $1; }; run",
    "start:prod": "node dist/apps/webhook/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pre-commit": "lint-staged",
    "pre-push": "jest",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/webhook/test/jest-e2e.json"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@google-cloud/cloud-sql-connector": "^1.2.1",
    "@google-cloud/secret-manager": "^5.0.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/typeorm": "^9.0.0",
    "@temporalio/activity": "^1.5.2",
    "@temporalio/client": "^1.5.2",
    "@temporalio/worker": "^1.5.2",
    "@temporalio/workflow": "^1.5.2",
    "axios": "^0.27.2",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "fast-xml-parser": "^4.3.5",
    "googleapis": "^134.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nest-commander": "^3.0.0",
    "nestjs-pino": "^3.1.1",
    "pg": "^8.11.3",
    "pino-http": "^8.2.0",
    "process": "^0.11.10",
    "prom-client": "^15.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "round": "^2.0.1",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.18",
    "ws": "^8.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/json-rules-engine": "^4.1.0",
    "@types/lodash": "^4.14.187",
    "@types/marked": "^5.0.0",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/pg": "^8.6.5",
    "@types/seedrandom": "^3.0.5",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "28.1.2",
    "lint-staged": "^13.0.3",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.74.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/hasura(|/.*)$": "<rootDir>/libs/hasura/src/$1",
      "^@app/logging(|/.*)$": "<rootDir>/libs/logging/src/$1",
      "^@app/temporal(|/.*)$": "<rootDir>/libs/temporal/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/bootstrap(|/.*)$": "<rootDir>/libs/bootstrap/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
